{
    "comment": "Houses config for custom components that you'd like to import into Finsemble.",
    "components": {
        "Finsemble Documentation": {
            "window": {
                "url": "http://documentation.chartiq.com/finsemble",
                "left": "center",
                "top": "center",
                "height": 800,
                "width": 1000
            },
            "component": {},
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": false
                    },
                    "Window Manager": {
                        "persistWindowState": false,
                        "FSBLHeader": true,
                        "showLinker": false
                    }
                }
            }
        },
        "Getting Started Tutorial": {
            "window": {
                "url": "https://documentation.chartiq.com/finsemble/tutorial-gettingStarted.html",
                "height": 800,
                "width": 1000
            },
            "component": {
                "spawnOnStartup": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": false
                    },
                    "Window Manager": {
                        "persistWindowState": true,
                        "FSBLHeader": true,
                        "showLinker": false
                    }
                }
            }
        },
        "Docking Move Mask": {
            "window": {
                "url": "$applicationRoot/components/dockingGroupMask/dockingGroupMask.html",
                "frame": false,
                "resizable": false,
                "showTaskbarIcon": false,
                "options": {
                    "opacity": 0.7,
                    "autoShow": false
                }
            },
            "component": {
                "category": "system"
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": false
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": false
                    },
                    "Window Manager": {
                        "FSBLHeader": false,
                        "persistWindowState": false
                    }
                }
            }
        },
        "windowTitleBar": {
            "window": {
                "url": "$applicationRoot/components/windowTitleBar/windowTitleBar.js",
                "top": "center",
                "left": "center",
                "width": 358,
                "height": 458,
                "ephemeral": true
            },
            "component": {
                "category": "system"
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": false
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": false
                    },
                    "Window Manager": {
                        "FSBLHeader": false,
                        "persistWindowState": false
                    }
                }
            }
        },
        "Welcome Component": {
            "window": {
                "url": "$applicationRoot/components/welcome/welcome.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 400,
                "height": 370
            },
            "component": {
                "inject": false,
                "spawnOnStartup": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "welcome"
                    }
                }
            }
        },
        "Notepad": {
            "window": {
                "id": "Notepad",
                "native": true,
                "path": "notepad.exe",
                "url": "",
                "defaultHeight": 600,
                "autoShow": true,
                "alwaysOnTop": false,
                "resizable": true,
                "showTaskbarIcon": false,
                "contextMenu": true,
                "addToWorkspace": true
            },
            "component": {
                "spawnOnHotkey": [
                    "ctrl",
                    "alt",
                    "n"
                ]
            },
            "foreign": {
                "services": {
                    "workspaceService": {
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "persistWindowState": false,
                        "FSBLHeader": true
                    },
                    "Toolbar": {
                        "iconURL": "$applicationRoot/components/assets/img/notepad.png"
                    }
                }
            }
        },
        "UserPreferences": {
            "window": {
                "url": "$applicationRoot/components/userPreferences/UserPreferences.html",
                "frame": false,
                "top": "center",
                "left": "center",
                "width": 800,
                "height": 600,
                "options": {
                    "autoShow": false,
                    "resizable": false,
                    "alwaysOnTop": true
                }
            },
            "component": {
                "category": "system",
                "spawnOnStartup": true
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": false,
                        "isArrangable": false
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": false
                    },
                    "Window Manager": {
                        "FSBLHeader": false,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "UserPreferences"
                    }
                }
            }
        },
        "rpc-test": {
            "window": {
                "url": "$applicationRoot/components/rpc-test/rpc-test.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 800,
                "height": 600
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "rpc-test"
                    }
                }
            }
        },
        "chart": {
            "window": {
                "url": "$applicationRoot/components/chart/sample-template-advanced.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 800,
                "height": 600
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true,
                        "showLinker": true
                    },
                    "Toolbar": {
                        "iconClass": "chart"
                    }
                }
            }
        },
        "iframeTest": {
            "window": {
                "url": "$applicationRoot/components/iframeTest/iframeTest.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": 800,
                "height": 600
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "iframeTest"
                    }
                }
            }
        }
    }
}
