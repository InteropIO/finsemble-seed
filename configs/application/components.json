{
	"comment": "Houses config for custom components that you'd like to import into Finsemble.",
	"components": {
		"Finsemble Documentation": {
			"window": {
				"url": "http://documentation.chartiq.com/finsemble",
				"left": "center",
				"top": "center",
				"height": 800,
				"width": 1000
			},
			"component": {},
			"foreign": {
				"services": {
					"dockingService": {
						"isArrangable": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": false
					},
					"Window Manager": {
						"persistWindowState": false,
						"FSBLHeader": true,
						"showLinker": false
					}
				}
			}
		},
		"Getting Started Tutorial": {
			"window": {
				"url": "https://documentation.chartiq.com/finsemble/tutorial-gettingStarted.html",
				"height": 800,
				"width": 1000
			},
			"component": {
				"spawnOnStartup": false
			},
			"foreign": {
				"services": {
					"dockingService": {
						"isArrangable": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": false
					},
					"Window Manager": {
						"persistWindowState": true,
						"FSBLHeader": true,
						"showLinker": false
					}
				}
			}
		},
		"Docking Move Mask": {
			"window": {
				"url": "$applicationRoot/components/dockingGroupMask/dockingGroupMask.html",
				"frame": false,
				"resizable": false,
				"showTaskbarIcon": false,
				"options": {
					"opacity": 0.7,
					"autoShow": false
				}
			},
			"component": {
				"category": "system"
			},
			"foreign": {
				"services": {
					"dockingService": {
						"isArrangable": false
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": false
					},
					"Window Manager": {
						"FSBLHeader": false,
						"persistWindowState": false
					}
				}
			}
		},
		"windowTitleBar": {
			"window": {
				"url": "$applicationRoot/components/windowTitleBar/windowTitleBar.js",
				"top": "center",
				"left": "center",
				"width": 358,
				"height": 458,
				"ephemeral": true
			},
			"component": {
				"category": "system"
			},
			"foreign": {
				"services": {
					"dockingService": {
						"isArrangable": false
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": false
					},
					"Window Manager": {
						"FSBLHeader": false,
						"persistWindowState": false
					}
				}
			}
		},
		"Welcome Component": {
			"window": {
				"url": "$applicationRoot/components/welcome/welcome.html",
				"frame": false,
				"resizable": true,
				"autoShow": true,
				"top": "center",
				"left": "center",
				"width": 400,
				"height": 400
			},
			"component": {
				"inject": false,
				"spawnOnStartup": false
			},
			"foreign": {
				"services": {
					"dockingService": {
						"canGroup": true,
						"isArrangable": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": true
					},
					"Window Manager": {
						"FSBLHeader": {
							"hideClose": true
						},
						"persistWindowState": true
					},
					"Toolbar": {
						"iconClass": "welcome"
					}
				}
			}
		}
	}
}