{
	"comment": "Sample component configuration",
	"components": {
		"Welcome Component": {
			"window": {
				"url": "$applicationRoot/components/welcome/welcome.html",
				"affinity": "workspaceComponents",
				"options": {
					"resizable": true,
					"autoShow": true,
					"alwaysOnTop": false
				},
				"top": "center",
				"left": "center",
				"width": 432,
				"height": 470
			},
			"component": {
				"displayName": "Welcome Component",
				"spawnOnStartup": false,
				"preload": [
					"$applicationRoot/preloads/zoom.js",
					"$applicationRoot/preloads/FDC3Client.js"
				]
			},
			"foreign": {
				"services": {
					"windowService": {
						"allowSnapping": true,
						"allowGrouping": true,
						"allowAutoArrange": true,
						"allowMinimize": true
					},
					"fdc3": {
						"intents": [
							{
								"name": "StartCall",
								"displayName": "Call",
								"contexts": [
									"fdc3.contact"
								]
							},
							{
								"name": "ViewChart",
								"displayName": "View Chart",
								"contexts": [
									"fdc3.instrument"
								]
							}
						]
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": true
					},
					"Window Manager": {
						"alwaysOnTopIcon": false,
						"showLinker": true,
						"FSBLHeader": true,
						"persistWindowState": true,
						"title": "Welcome to Finsemble"
					},
					"Toolbar": {
						"iconClass": "ff-component",
						"iconURL": "$applicationRoot/assets/img/Finsemble_Taskbar_Icon.png"
					}
				}
			}
		},
		"nonConfiguredComponent": {
			"window": {
				"url": "$applicationRoot/components/nonConfiguredComponent/nonConfiguredComponent.html",
				"affinity": "systemComponents",
				"options": {
					"resizable": true,
					"autoShow": true
				},
				"top": "center",
				"left": "center",
				"width": 400,
				"height": 432
			}
		},
		"Notepad": {
			"window": {
				"windowType": "assimilation",
				"path": "notepad.exe",
				"defaultHeight": 600,
				"options": {
					"autoShow": true
				},
				"showTaskbarIcon": false,
				"contextMenu": true,
				"addToWorkspace": true
			},
			"component": {
				"spawnOnHotkey": [
					"ctrl",
					"alt",
					"n"
				]
			},
			"foreign": {
				"services": {
					"windowService": {
						"allowAutoArrange": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": true
					},
					"Window Manager": {
						"persistWindowState": false,
						"FSBLHeader": true
					},
					"Toolbar": {
						"iconURL": "$applicationRoot/assets/img/notepad.png"
					}
				}
			}
		},
		"Getting Started Tutorial": {
			"window": {
				"url": "https://www.chartiq.com/tutorials/?slug=finsemble",
				"height": 800,
				"width": 1000
			},
			"component": {
				"spawnOnStartup": false
			},
			"foreign": {
				"services": {
					"workspaceService": {
						"persistURL": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": false
					},
					"Window Manager": {
						"showLinker": false,
						"FSBLHeader": true,
						"persistWindowState": true,
						"title": "Finsemble Getting Started Tutorial"
					}
				}
			}
		},
		"Finsemble Documentation": {
			"window": {
				"url": "http://documentation.chartiq.com/finsemble",
				"left": "center",
				"top": "center",
				"height": 800,
				"width": 1000
			},
			"component": {},
			"foreign": {
				"services": {
					"workspaceService": {
						"persistURL": true
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": false
					},
					"Window Manager": {
						"showLinker": false,
						"FSBLHeader": true,
						"persistWindowState": true,
						"title": "Finsemble Documentation"
					}
				}
			}
		},
		"defaultAuthentication": {
			"window": {
				"url": "$applicationRoot/components/authentication/defaultAuthentication.html",
				"top": "center",
				"left": "center",
				"width": 358,
				"height": 658,
				"ephemeral": true,
				"options": {
					"autoShow": false
				}
			},
			"component": {
				"neededClients": [
					"LauncherClient",
					"AuthenticationClient",
					"StorageClient"
				],
				"category": "system"
			},
			"foreign": {
				"services": {
					"windowService": {
						"allowAutoArrange": false
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": false
					},
					"Window Manager": {
						"FSBLHeader": false,
						"persistWindowState": false
					}
				}
			}
		},
		"FDC3Tester": {
			"window": {
				"url": "$applicationRoot/components/FDC3Tester/FDC3Tester.html",
				"options": {
					"resizable": true,
					"autoShow": true,
					"alwaysOnTop": false
				},
				"top": "center",
				"left": "center",
				"width": 800,
				"height": 600
			},
			"component": {
				"preload": "$applicationRoot/preloads/FDC3Client.js",
				"spawnOnStartup": false,
				"singleton": false,
				"showDevConsoleOnVisible": false
			},
			"foreign": {
				"services": {
					"windowService": {
						"manageWindowMovement": true,
						"allowAutoArrange": true,
						"allowSnapping": true,
						"allowTabbing": true,
						"allowTiling": true,
						"allowGrouping": true,
						"allowMinimize": true
					},
					"fdc3": {
						"intents": [
							{
								"name": "fdc3.instrument",
								"displayName": "ViewChart",
								"contexts": [
									"fdc3.contact"
								]
							}
						]
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": true
					},
					"Window Manager": {
						"alwaysOnTopIcon": true,
						"title": "FDC3Tester",
						"persistWindowState": true
					},
					"Toolbar": {
						"iconClass": "FDC3Tester",
						"iconURL": "http://localhost:3375/assets/img/Finsemble_Taskbar_Icon.png"
					}
				}
			}
		}
	}
}